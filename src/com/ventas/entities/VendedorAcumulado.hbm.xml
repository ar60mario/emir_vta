<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ventas.entities.VendedorAcumulado" schema="vta_emir" table="vendedor_acumulado">
        <id name="id" column="ID">
            <generator class="identity"/>
        </id>
        <property name="importe" type="double" column="IMPORTE"/>
        <property name="fechaUltimoMovimiento" type="date" column="FECHA_ULTIMO_MOVIMIENTO"/>
        
    </class>
</hibernate-mapping>