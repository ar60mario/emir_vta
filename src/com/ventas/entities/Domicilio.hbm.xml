<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ventas.entities.Domicilio" schema="vta_emir" table="domicilio">
        <id name="id" column="ID">
            <generator class="identity"/>
        </id>
        <property name="calle" type="integer" column="CALLE"/>
        <property name="numero" type="string" column="NUMERO"/>
        <property name="pisoDto" type="boolean" column="PISO_DTO"/>
        <property name="codigoPostal" type="boolean" column="CODIGO_POSTAL"/>
        <property name="localidad" type="boolean" column="LOCALIDAD"/>
        <property name="provincia" type="boolean" column="PROVINCIA"/>
        
    </class>
</hibernate-mapping>